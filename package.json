  "homepage": "https://github.com/zmzhoi/create-react-app#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zmzhoi/create-react-app.git"
  },
  "bugs": {
    "url": "https://github.com/zmzhoi/create-react-app/issues"
  },
  "scripts": {
    "lint": "eslint packages --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --check \"**/*.(ts|tsx|js|jsx|json|md)\"",
    "format:fix": "prettier --write \"**/*.(ts|tsx|js|jsx|json|md)\"",
    "changeset": "changeset",
    "version": "changeset version",
    "release": "changeset publish",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint": "^8.28.0",
    "prettier": "^2.8.0",
    "@babel/core": "^7.20.2",
    "@babel/runtime": "^7.20.1",
    "@changesets/cli": "^2.25.2",
    "typescript": "^4.9.3",
    "husky": "^8.0.0"
  },
  "workspaces": [
    "packages/*",
    "examples/*"
  ]
}
